@using Blazored.LocalStorage
@inject HttpClient _client

@if (UserSession.PublicServers != null)
{
    @foreach (ServerData server in UserSession.PublicServers )
    {
        <ServerComponent ServerData="@server" />
    }
}
@code
{

    [CascadingParameter]
    public SessionManager UserSession{ get; set; }
}
