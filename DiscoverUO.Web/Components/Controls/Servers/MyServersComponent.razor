@using System.Net
@using System.Net.Http;
@using System.Net.Http.Headers;
@using DiscoverUO.Lib.Shared
@using DiscoverUO.Lib.Shared.Contracts
@inject ISnackbar snackbar
@inject NavigationManager navManager
@inject HttpClient client

@if (UserSession.PublicServers != null)
{
    @foreach( ServerData server in UserSession.UserOwnedServers )
    {
        <ServerComponent ServerData="@server" />
    }
}
@code
{
    [CascadingParameter]
    public SessionManager UserSession { get; set; }
}