@using DiscoverUO.Lib.Shared.Servers
@using DiscoverUO.Web.Components.Controls
@using Blazored.LocalStorage
@using System.Net
@inject ILocalStorageService _localStorage
@inject HttpClient _client

@page "/servers"

<PageTitle>Servers</PageTitle>

<MudContainer>
    <ServerListComponent></ServerListComponent>
</MudContainer>

@code{

    public List<ServerData> _serverlist{ get; set; }

    protected override async Task OnInitializedAsync()
    {
        _serverlist = await DataManager.GetPublicServers(_client, _localStorage);
        base.OnInitializedAsync();
    }
}