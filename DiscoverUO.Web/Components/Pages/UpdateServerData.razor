@inject HttpClient client
@inject NavigationManager navManager
@inject ISnackbar snackbar
@page "/servers/update"

<PageTitle>Update Server</PageTitle>

@if (UserSession.UserAuthenticated)
{
    <MudContainer>
        <UpdateServerComponent />
    </MudContainer>
}
else
{
    snackbar.Add("You must be logged in to add to your favorites list.", Severity.Error);
    navManager.NavigateTo("/signin");
}

@code
{
    [CascadingParameter]
    public SessionManager UserSession { get; set; }

}