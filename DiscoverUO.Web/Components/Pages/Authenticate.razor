@page "/Authenticate"
@inject AuthenticationService AuthService

@code {
    private string? token;

    protected override async Task OnInitializedAsync()
    {
        token = await AuthService.Login(new LoginDto());
    }
}

@if (token == null)
{
    <p><em>Loading...</em></p>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>// Add other columns as needed</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in token)
            {
                <tr>
                    <td>@item</td>

                    <td>// Add other columns as needed</td>
                </tr>
            }
        </tbody>
    </table>
}